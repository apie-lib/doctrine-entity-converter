{
  "name": "apie/doctrine-entity-converter",
  "description": "Composer package of the apie library: doctrine entity converter",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\DoctrineEntityConverter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\DoctrineEntityConverter\\": "tests/"
    }
  },
  "conflict": {
    "nette/utils": "3.*"
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "apie/storage-metadata-builder": "self.version",
    "doctrine/orm": "^2.19",
    "nette/php-generator": "^4.1.5",
    "symfony/finder": "^6.4"
  },
  "require-dev": {
    "apie/common-value-objects": "self.version",
    "apie/service-provider-generator": "0.11.2",
    "apie/text-value-objects": "self.version",
    "apie/fixtures": "self.version",
    "doctrine/doctrine-bundle": ">=2.8",
    "illuminate/support": "*",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0",
    "symfony/cache": "^6.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}